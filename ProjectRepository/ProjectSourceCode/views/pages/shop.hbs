            const rows = document.querySelectorAll('tbody tr'); // Get all product rows

            // Loop through all rows
            rows.forEach(row => {
                const productName = row.querySelector('td:nth-child(2)').textContent.toLowerCase(); // Get product name
                const productDescription = row.querySelector('td:nth-child(3)').textContent.toLowerCase(); // Get product description

                // Check if the search query matches the name or description
                if (productName.includes(query) || productDescription.includes(query)) {
                    row.style.display = ''; // Show row if it matches
                } else {
                    row.style.display = 'none'; // Hide row if it doesn't match
                }
            });
        });
